{
  "rewrites": [
    {
      "source": "/api/cron",
      "destination": "/api/cron.js"
    },
    {
      "source": "/api/webhook",
      "destination": "/api/webhook.js"
    },
    {
      "source": "/((?!api/|_next/|static/|.*\\..*).*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/cron?type=weekly",
      "schedule": "0 1 * * 1"
    }
  ]
}